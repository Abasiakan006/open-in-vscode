{
  "name": "Atlassian to VS Code",
  "version": "0.4.1",
  "description": "Chrome browser extension that adds a button to navigate from Jira and Bitbucket to VS Code",
  "manifest_version": 2,
  "author": "Timmo Verlaan",
  "homepage_url": "https://github.com/tverlaan/open-in-vscode/tree/support/v0.4",
  "page_action": {
    "default_icon": {
      "16": "images/icon_atlas_vscode_16.png",
      "24": "images/icon_atlas_vscode_24.png",
      "32": "images/icon_atlas_vscode_32.png"
    },
    "default_title": "Atlas to VS Code"
  },
  "permissions": [
    "declarativeContent",
    "https://bitbucket.org/",
    "https://*.atlassian.net/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/icon_atlas_vscode_16.png",
    "48": "images/icon_atlas_vscode_48.png",
    "128": "images/icon_atlas_vscode_128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+O",
        "mac": "Command+Shift+O",
        "chromeos": "Ctrl+Shift+O",
        "linux": "Ctrl+Shift+O"
      }
    }
  }
}